{
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier",
      "react"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true
    },
    "rules": {
      "prettier/prettier": "warn",
      "react/jsx-uses-vars": "warn"
    }
  }
